#include <stdio.h>
int main()
{
    int a = 203;
    if(a > 0)
    {
        int i;
        int j;
        i = 0;
        while(i < a)
        {
            j = 0;
            while(j <= i)
            {
                printf("*");
                j++;
                
            }
         printf("\n");   
         i++;   
        }
    }
    
}
